<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>CreationContact</servlet-name>
        <servlet-class>com.contactAdresse.servlets.CreationContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreationAdresse</servlet-name>
        <servlet-class>com.contactAdresse.servlets.CreationAdresse</servlet-class>
    </servlet>
     
    <servlet-mapping>
        <servlet-name>CreationContact</servlet-name>
        <url-pattern>/creationContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreationAdresse</servlet-name>
        <url-pattern>/creationAdresse</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
		<welcome-file>/WEB-INF/creerContact.jsp</welcome-file>
	</welcome-file-list>
</web-app>